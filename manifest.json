{
  "manifest_version": 2,
  "name": "ClipeGoogleDriveImage",
  "description": "GoogleDriveの画像をimgタグで貼れるようにするやつ",
  "version": "1.0",
  "browser_action": {

  },
  "permissions": [
    "tabs","clipboardWrite"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },

  "commands": {
    "save_url_and_title": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "Save Title and URL"
    }
  },
  "permissions": [
    "tabs","clipboardWrite"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "options_page": "options.html"
}
